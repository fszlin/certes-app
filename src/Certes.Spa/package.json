{
  "name": "certes-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "alphanum-sort": "^1.0.2",
    "axios": "^0.18.0",
    "bootstrap": "^4.1.1",
    "formik": "^0.11.11",
    "msal": "^0.1.7",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "2.16.0",
    "reactstrap": "^6.1.0"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js start-api",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "start-js": "react-scripts-ts start",
    "build-css": "node-sass-chokidar src/ -o src/",
    "build-js": "react-scripts-ts build",
    "watch-css": "node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-api": "cd ../Certes.Api/ && func start",
    "postinstall": "node lo0.js"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.3.3",
    "@types/react": "^16.3.18",
    "@types/react-dom": "^16.0.6",
    "@types/react-router": "^4.0.26",
    "@types/react-router-dom": "^4.2.7",
    "@types/reactstrap": "^5.0.27",
    "azure-functions-core-tools": "2.0.1-beta.29",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "typescript": "^2.9.2"
  }
}
